{
  "singleToken": {
    "description": "Send notification to a single FCM token",
    "endpoint": "http://localhost:5555/eventbooking-fd7c2/us-central1/testSendNotificationToToken",
    "method": "POST",
    "body": {
      "fcmToken": "YOUR_FCM_TOKEN_HERE",
      "title": "Test Notification",
      "body": "This is a test message from Firebase Functions",
      "type": "general"
    }
  },
  "multipleTokens": {
    "description": "Send notification to multiple FCM tokens",
    "endpoint": "http://localhost:5555/eventbooking-fd7c2/us-central1/testSendNotificationToToken",
    "method": "POST",
    "body": {
      "fcmTokens": [
        "TOKEN_1",
        "TOKEN_2",
        "TOKEN_3"
      ],
      "title": "Batch Notification",
      "body": "This notification is sent to multiple devices",
      "type": "general"
    }
  },
  "eventReminder": {
    "description": "Send event reminder notification",
    "endpoint": "http://localhost:5555/eventbooking-fd7c2/us-central1/testSendNotificationToToken",
