      "fcmToken": "YOUR_FCM_TOKEN_HERE",
      "title": "Event Reminder",
      "body": "Your event 'Beach Party' starts in 1 hour!",
      "type": "event_reminder",
      "eventId": "event_123",
      "actionUrl": "whatsthemove://events/event_123"
    }
  },
  "registrationDeadline": {
    "description": "Send registration deadline notification",
    "endpoint": "http://localhost:5555/eventbooking-fd7c2/us-central1/testSendNotificationToToken",
